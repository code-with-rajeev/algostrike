{
    "version": 2,
    "builds": [
        {
            "src": "myproject/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLamdaSize": "15mb", "runtime": "python3.11" }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "myproject/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}