{
    "ignoreFiles": [
        "backend_app/**",
        "kotak_neo/**",
        "server_requirements.txt"
    ],
    "builds": [{
        "src": "myproject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "myproject/wsgi.py"
        }
    ]
    
}